{
  "name": "@berthcare/backend",
  "version": "2.0.0",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:connection": "tsx src/test-connection.ts",
    "test:s3": "tsx src/storage/test-s3.ts",
    "migrate:up": "tsx src/db/migrate.ts up",
    "migrate:down": "tsx src/db/migrate.ts down",
    "db:reset": "tsx src/db/migrate.ts down all && tsx src/db/migrate.ts up",
    "db:verify": "tsx src/db/verify-schema.ts",
    "db:seed": "tsx src/db/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.907.0",
    "@aws-sdk/s3-request-presigner": "^3.907.0",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@sentry/node": "^7.99.0",
    "@sentry/profiling-node": "^1.3.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "winston": "^3.11.0",
    "connect-redis": "^7.1.1",
    "express-session": "^1.17.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.7",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "node-pg-migrate": "^7.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
